{
  "custom/dunst-notification": {
    "tooltip": true,
    "return-type": "json",
    "exec": "$HOME/.local/bin/scripts/window_manager/dmenu-dunst.py -c",
    "on-click": "$HOME/.local/bin/scripts/window_manager/dmenu-dunst.py -s",
    "on-click-right": "$HOME/.local/bin/scripts/window_manager/dmenu-dunst.py -d",
    "on-click-middle": "$HOME/.local/bin/scripts/window_manager/dmenu-dunst.py -r",
    "interval": 3,
    "escape": true,
    // "hide-empty-text": true,
    "format": "{icon}",
    "format-icons": {
      "notification": "󰂚<sup><span foreground='{{ colors.error.default.hex }}'></span></sup>",
      "none": "󰂚",
      "dnd-notification": "󰂛<sup><span foreground='{{ colors.error.default.hex }}'></span></sup>",
      "dnd-none": "󰂛"
    }
  }
}
